module "htpasswd_idp" {
  source = "terraform-redhat/rosa-hcp/rhcs//modules/idp"
  version = "1.7.0"

  cluster_id         = module.rosa_hcp.cluster_id
  name               = var.idp_name
  idp_type           = "htpasswd"
  htpasswd_idp_users = [
    {
      username = var.idp_username
      password = var.idp_password
    }
  ]
}